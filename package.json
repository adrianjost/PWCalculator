{
	"name": "calculator",
	"version": "1.2.0",
	"description": "a calculator webapp",
	"author": "Adrian Jost <github@adrianjost.dev>",
	"private": true,
	"scripts": {
		"dev": "parcel index.html --port 8080",
		"build": "parcel build index.html",
		"deploy": "npm run build && firebase deploy",
		"lint:all:prettier": "prettier --loglevel log -c \"**/*.{js,json,css,scss,vue,html,md}\" --write",
		"lint:fix:prettier": "npm run lint:all:prettier --write",
		"lint:ci": "run-s lint:all:*",
		"lint": "run-s lint:fix:*"
	},
	"dependencies": {
		"string-math": "^1.2.2",
		"vue": "^2.7.12",
		"vue-hot-reload-api": "^2.3.4"
	},
	"devDependencies": {
		"@vue/component-compiler-utils": "^3.3.0",
		"autoprefixer": "^9.8.6",
		"babel-core": "^6.26.3",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-2": "^6.24.1",
		"firebase-tools": "^11.14.4",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.5",
		"parcel-plugin-sw-cache": "^0.3.1",
		"parcel-plugin-web-extension": "^1.6.1",
		"prettier": "2.7.1",
		"sass": "^1.55.0",
		"vue-template-compiler": "^2.7.12"
	},
	"engines": {
		"node": ">= 12",
		"npm": ">= 7"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"cache": {
		"disablePlugin": false,
		"inDev": true,
		"strategy": "default",
		"clearDist": true
	}
}
